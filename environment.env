# PostgreSQL Ayarları
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n_password_123
POSTGRES_DB=n8n

# n8n Temel Kimlik Doğrulama
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# n8n Genel Ayarları
GENERIC_TIMEZONE=Europe/Istanbul
N8N_HOST=localhost
WEBHOOK_URL=http://localhost:5678/

# n8n Güvenlik ve Performans Ayarları
N8N_RUNNERS_ENABLED=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_SECURE_COOKIE=false
N8N_METRICS=false

# Güvenlik Notu: Üretim ortamında bu şifreleri değiştirin!
# Üretim için öneriler:
# - N8N_SECURE_COOKIE=true (HTTPS kullanırken)
# - N8N_BLOCK_ENV_ACCESS_IN_NODE=true (güvenlik için)
# - Güçlü şifreler kullanın
